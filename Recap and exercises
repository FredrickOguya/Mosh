# students = ['Fred', 'Liam', 'Vivian', 'Brian', 'Alice', 'Mark', 'Diana']
# Write code that:

# Finds all students whose names start with "D".

# Stores them in a new list.

# Prints that list.

# When youâ€™re done, share your code!

students = ['Fred', 'Liam', 'Vivian', 'Brian', 'Alice', 'Mark', 'Diana']

print("Brian" in students)

print("George" in students)

print("Alice" in students)


new_list = []
for student in students:
    if student.startswith("D"):
        new_list.append(student)

print(new_list)